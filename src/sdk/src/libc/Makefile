# src/trio/trio.c \
# src/trio/trionan.c \
# src/trio/triostr.c \

START=src/start.c
STARTO= $(START:.c=.o)
OBJO = $(OBJS:.c=.o)
OBJSO= $(ASMOBJS:.S=.o)
OBJ=libc.a crt
CC=gcc -w -m32 -Wa,--32  -g -nostdlib -nostdinc -trigraphs  -I $(SDKDIR)/include -c
SDKDIR=../..

all: $(OBJ)

libc.a: $(OBJO) $(OBJSO)
	ar rcs $@ *.o
	cp libc.a $(SDKDIR)/lib/libc.a

crt: $(STARTO)
	cp start.o $(SDKDIR)/lib/crt_c.o
	rm *.o

%.o: %.c
	$(CC) $<

%.o: %.S
	$(CC) $<

clean:
	rm -f $(OBJ) *.o *.a
